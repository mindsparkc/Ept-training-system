<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.etp.cp.courseware.mapper.TCoursewareCompanyWithCpMapper">

    <resultMap type="TCoursewareCompanyWithCp" id="TCoursewareCompanyResult">
        <result property="id"    column="id"    />
        <result property="kjid"    column="kjid"    />
        <result property="qyid"    column="qyid"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectTCoursewareCompanyVo">
        SELECT id, kjid, qyid, del_flag, create_by, create_time, update_by, update_time, remark FROM t_courseware_company
    </sql>

    <select id="selectTCoursewareCompanyList" parameterType="TCoursewareCompanyWithCp" resultMap="TCoursewareCompanyResult">
        <include refid="selectTCoursewareCompanyVo"/>
        <where>
            <if test="kjid != null "> AND kjid = #{kjid}</if>
            <if test="qyid != null "> AND qyid = #{qyid}</if>
        </where>
    </select>

    <select id="selectTCoursewareCompanyById" parameterType="Long" resultMap="TCoursewareCompanyResult">
        <include refid="selectTCoursewareCompanyVo"/>
        WHERE id = #{id}
    </select>

    <insert id="insertTCoursewareCompany" parameterType="TCoursewareCompanyWithCp" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO t_courseware_company
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="kjid != null">kjid,</if>
            <if test="qyid != null">qyid,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="kjid != null">#{kjid},</if>
            <if test="qyid != null">#{qyid},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateTCoursewareCompany" parameterType="TCoursewareCompanyWithCp">
        UPDATE t_courseware_company
        <trim prefix="SET" suffixOverrides=",">
            <if test="kjid != null">kjid = #{kjid},</if>
            <if test="qyid != null">qyid = #{qyid},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        WHERE id = #{id}
    </update>

    <delete id="deleteTCoursewareCompanyById" parameterType="Long">
        DELETE FROM t_courseware_company WHERE id = #{id}
    </delete>

    <delete id="deleteTCoursewareCompanyByIds" parameterType="String">
        DELETE FROM t_courseware_company WHERE id IN
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <delete id="deleteTCoursewareCompanyByKjId">
        <!--
        UPDATE t_courseware_company SET del_flag = '1'
        WHERE kjid = #{id}
        修改为物理删除
        -->
        DELETE FROM t_courseware_company WHERE kjid = #{id}
    </delete>
</mapper>
